<div class="container">
  <h1>Editor dashboard</h1>
  <%= search_form_for @q do |f| %>
  <%= f.search_field :body_cont %>
  <%= f.submit name: ''%>
  <% end %>

<div class="row">
      <table class="table table-striped">
        <tr>
            <%= render @posts %>
        </tr>
      </table>
</div>
<div class="py-4">
    <%= link_to 'New post', new_post_path, class: 'btn btn-primary btn-lg' %>
</div>


</div>
